<style>
	body {
	  font-family: "Lato", sans-serif;
	}
	
	.sidenav {
	  height: 100%;
	  width: 0;
	  position: fixed;
	  z-index: 1;
	  top: 0;
	  left: 0;
	  background-color: rgb(52, 52, 52);
	  overflow-x: hidden;
	  transition: 0.5s;
	  padding-top: 60px;
	}
	
	.sidenav a {
	  padding: 8px 8px 8px 32px;
	  text-decoration: none;
	  font-size: 25px;
	  color: rgb(255,0,0);
	  display: block;
	  transition: 0.3s;
	}
	
	.sidenav a:hover {
	  color: #f1f1f1;
	}
	
	.sidenav .closebtn {
	  position: absolute;
	  top: 0;
	  right: 25px;
	  font-size: 36px;
	  margin-left: 50px;
	}
	
	@media screen and (max-height: 450px) {
	  .sidenav {padding-top: 30px;}
	  .sidenav a {font-size: 36px;}
	}
	</style>
  
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
<table class='table table-hover table-condensed'>
	
	<% diff.forEach(function(diff) { %>

	<tr>
	<td><%- diff.qn %></td>
	<td><%- diff.answer1 %></td> 
    <td><%- diff.answer2 %></td> 
    <td><%- diff.answer3 %></td> 
    <td><%- diff.answer4 %></td>
    <td><%- diff.correct_answer %></td> 
    <td><%- diff.category %></td>
    <td><%- diff.answer_description %></td> 






		<td>
			<form action=<%= "/app\?question_id\=" + diff.id + "\&user_id\=2"  %> method ='post'>			
				<button type="submit" value="Submit">Approve</button>
			</form>
		</td>
	 </tr> 
  
	<% }); %>
	
	</table>