	<div class="container">
		<div class="row animate-box">
			<div class="col-md-6 col-md-offset-3 text-center fh5co-heading">
				
	<table class='table table-hover table-condensed'>
	
		<% diff.forEach(function(diff) { %>
	
		<tr>
		<td><%- diff.qn %></td>
		<td><%- diff.answer1 %></td> 
		<td><%- diff.answer2 %></td> 
		<td><%- diff.answer3 %></td> 
		<td><%- diff.answer4 %></td>
		<td><%- diff.correct_answer %></td> 
		<td><%- diff.category %></td>
		<td><%- diff.answer_description %></td> 
	
	
	
	
	
	
			<td>
				<form action=<%= "/app\?question_id\=" + diff.id + "\&user_id\=" + user_id  %> method ='post'>			
					<button type="submit" value="Submit">Approve</button>
				</form>
				<form action=<%="/deny-questions\?question_id=" + diff.id +"\&user_id=" + user_id %> method='POST'>
					<button typr="submit" value="Submit">Deny</button>
				</form>
	
			</td>
			
		 </tr> 
	  
		<% }); %>
		
		</table>
